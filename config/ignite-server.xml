                                <property name="aliases">
                                    <map>
                                        <entry key="schedName" value="SCHED_NAME"/>
                                        <entry key="entryId" value="ENTRY_ID"/>
                                        <entry key="triggerName" value="TRIGGER_NAME"/>
                                        <entry key="triggerGroup" value="TRIGGER_GROUP"/>
