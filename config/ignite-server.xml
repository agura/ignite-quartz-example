                                        <bean class="org.apache.ignite.cache.QueryIndex">
                                            <property name="name" value="PRIMARY"/>
                                            <property name="indexType" value="SORTED"/>

                                            <property name="fields">
                                                <map>
                                                    <entry key="JOB_GROUP" value="true"/>
                                                    <entry key="SCHED_NAME" value="true"/>
                                                    <entry key="JOB_NAME" value="true"/>
                                                </map>
                                            </property>
                                        </bean>
                                    </list>
                                </property>
                            </bean>

                            <bean class="org.apache.ignite.cache.QueryEntity">
                                <property name="keyType" value="QRTZ_LOCKSKey"/>
                                <property name="valueType" value="QRTZ_LOCKS"/>

                                <property name="keyFields">
                                    <set>
                                        <value>SCHED_NAME</value>
                                        <value>LOCK_NAME</value>
                                    </set>
                                </property>

                                <property name="fields">
                                    <map>
                                        <entry key="SCHED_NAME" value="java.lang.String"/>
                                        <entry key="LOCK_NAME" value="java.lang.String"/>
                                    </map>
                                </property>

                                <property name="indexes">
                                    <list>
                                        <bean class="org.apache.ignite.cache.QueryIndex">
                                            <property name="name" value="PRIMARY"/>
                                            <property name="indexType" value="SORTED"/>

                                            <property name="fields">
                                                <map>
                                                    <entry key="LOCK_NAME" value="true"/>
                                                    <entry key="SCHED_NAME" value="true"/>
                                                </map>
                                            </property>
                                        </bean>
                                    </list>
                                </property>
