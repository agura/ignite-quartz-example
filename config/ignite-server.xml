                                        <entry key="entryId" value="java.lang.String"/>
                                        <entry key="triggerName" value="java.lang.String"/>
                                        <entry key="triggerGroup" value="java.lang.String"/>
                                        <entry key="instanceName" value="java.lang.String"/>
