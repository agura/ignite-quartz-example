                            <bean class="org.apache.ignite.cache.QueryEntity">
                                <property name="keyType" value="QRTZ_PAUSED_TRIGGER_GRPSKey"/>
                                <property name="valueType" value="QRTZ_PAUSED_TRIGGER_GRPS"/>

                                <property name="keyFields">
                                    <set>
                                        <value>schedName</value>
