                                        <bean class="org.apache.ignite.cache.QueryIndex">
                                            <property name="name" value="PRIMARY"/>
                                            <property name="indexType" value="SORTED"/>

                                            <property name="fields">
                                                <map>
                                                    <entry key="INSTANCE_NAME" value="true"/>
                                                    <entry key="SCHED_NAME" value="true"/>
                                                </map>
                                            </property>
                                        </bean>
                                    </list>
                                </property>
                            </bean>

                            <bean class="org.apache.ignite.cache.QueryEntity">
                                <property name="keyType" value="QRTZ_SIMPLE_TRIGGERSKey"/>
                                <property name="valueType" value="QRTZ_SIMPLE_TRIGGERS"/>

                                <property name="keyFields">
                                    <set>
                                        <value>SCHED_NAME</value>
                                        <value>TRIGGER_NAME</value>
                                        <value>TRIGGER_GROUP</value>
                                    </set>
                                </property>

                                <property name="fields">
                                    <map>
                                        <entry key="SCHED_NAME" value="java.lang.String"/>
                                        <entry key="TRIGGER_NAME" value="java.lang.String"/>
                                        <entry key="TRIGGER_GROUP" value="java.lang.String"/>
                                        <entry key="REPEAT_COUNT" value="java.lang.Integer"/>
                                        <entry key="REPEAT_INTERVAL" value="java.lang.Long"/>
                                        <entry key="TIMES_TRIGGERED" value="java.lang.Integer"/>
                                    </map>
                                </property>

                                <property name="indexes">
                                    <list>
                                        <bean class="org.apache.ignite.cache.QueryIndex">
                                            <property name="name" value="PRIMARY"/>
                                            <property name="indexType" value="SORTED"/>

                                            <property name="fields">
                                                <map>
                                                    <entry key="SCHED_NAME" value="true"/>
                                                    <entry key="TRIGGER_NAME" value="true"/>
                                                    <entry key="TRIGGER_GROUP" value="true"/>
                                                </map>
                                            </property>
                                        </bean>
                                    </list>
                                </property>
                            </bean>

                            <bean class="org.apache.ignite.cache.QueryEntity">
                                <property name="keyType" value="QRTZ_SIMPROP_TRIGGERSKey"/>
                                <property name="valueType" value="QRTZ_SIMPROP_TRIGGERS"/>

                                <property name="keyFields">
                                    <set>
